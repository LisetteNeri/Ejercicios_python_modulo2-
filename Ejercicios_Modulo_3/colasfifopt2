class QueueError(IndexError):
    pass

class Queue:
    def __init__(self):
        self.queue = []

    def put(self, elem):
        # Agregar al inicio de la lista
        self.queue.insert(0, elem)

    def get(self):
        # Sacar del final de la lista
        if len(self.queue) > 0:
            elem = self.queue[-1]
            del self.queue[-1]
            return elem
        else:
            raise QueueError("Cola vacía")

class SuperQueue(Queue):
    def isempty(self):
        # Devuelve True si la cola está vacía, False en caso contrario
        return len(self.queue) == 0


# Ejemplo de uso
que = SuperQueue()
que.put(1)
que.put("perro")
que.put(False)

for i in range(4):
    if not que.isempty():
        print(que.get())
    else:
        print("Cola vacía")
